{
    "name": "cortex/taggable",
    "type": "cortex-module",
    "description": "Cortex Taggable is a frontend layer for the polymorphic Laravel package, for tag management. You can tag any eloquent model with ease, and utilize the awesomeness of Sluggable, and Translatable models out of the box.",
    "keywords": [
        "tag",
        "model",
        "cortex",
        "laravel",
        "taggable",
        "taxonomy",
        "eloquent",
        "translatable",
        "polymorphic",
        "sluggable",
        "sortable",
        "rinvex"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-taggable/issues",
        "source": "https://github.com/rinvex/cortex-taggable",
        "docs": "https://github.com/rinvex/cortex-taggable/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-taggable/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "barryvdh/laravel-snappy": "^0.4.0",
        "cortex/foundation": "dev-develop",
        "davejamesmiller/laravel-breadcrumbs": "^4.0.0",
        "Illuminate/auth": "~5.5.0",
        "Illuminate/console": "~5.5.0",
        "Illuminate/http": "~5.5.0",
        "Illuminate/routing": "~5.5.0",
        "Illuminate/support": "~5.5.0",
        "laravelcollective/html": "^5.4.0",
        "league/fractal": "^0.16.0",
        "proengsoft/laravel-jsvalidation": "dev-master",
        "rinvex/composable": "dev-develop",
        "rinvex/fort": "dev-develop",
        "rinvex/support": "dev-develop",
        "rinvex/taggable": "dev-develop",
        "spatie/laravel-activitylog": "^2.0.0",
        "spatie/laravel-menu": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Cortex\\Taggable\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Taggable\\Providers\\TaggableServiceProvider"
            ]
        },
        "cortex-module": {
            "autoinstall": false,
            "icon": {
                "color": "#2196f3",
                "name": "heartbeat",
                "background-size": "cover",
                "background-color": "#e3f2fd",
                "background-position": "center",
                "background-repeat": "no-repeat"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
